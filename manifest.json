{
  "manifest_version": 3,
  "name": "디자인 오버레이 비교",
  "version": "1.0",
  "description": "현재 페이지와 디자인 이미지를 비교하여 시각적 차이를 확인합니다.",
  "permissions": ["activeTab", "scripting", "tabs", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["content.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
